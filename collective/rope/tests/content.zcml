<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five">

  <five:registerClass
      class=".portalcontent.RopePortalContent"
      meta_type="Rope Portal Content"
      permission="cmf.AddPortalContent"
      />

  <five:registerClass
      class=".atcontent.RopeATContent"
      meta_type="Rope AT Content"
      permission="cmf.AddPortalContent"
      />

  <utility
      component=".portalcontent.RopeFactory"
      name="rope.portalcontent"
      />

  <utility
      component=".atcontent.RopeATFactory"
      name="rope.atcontent"
      />

</configure>
